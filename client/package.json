    "@ffmpeg/ffmpeg": "^0.10.1",
    "@reduxjs/toolkit": "^1.8.0",
    "firebase": "^9.6.11",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-redux": "^7.2.6",
    "react-router-dom": "^6.2.2",
    "redux": "^4.1.2",
    "save-dev": "^0.0.1-security",
    "uuid": "^8.3.2"
  }
}
